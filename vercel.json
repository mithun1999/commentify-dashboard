{
  "redirects": [
    {
      "source": "/auth/sign-in",
      "destination": "/sign-in",
      "permanent": true
    },
    {
      "source": "/auth/sign-in/",
      "destination": "/sign-in",
      "permanent": true
    },
    {
      "source": "/auth/sign-up",
      "destination": "/sign-up",
      "permanent": true
    },
    {
      "source": "/auth/sign-up/",
      "destination": "/sign-up",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}