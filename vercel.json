{
  "redirects": [
    {
      "source": "/auth/sign-in",
      "destination": "/sign-in",
      "permanent": true
    },
    {
      "source": "/auth/sign-in/",
      "destination": "/sign-in",
      "permanent": true
    },
    {
      "source": "/auth/sign-up",
      "destination": "/sign-up",
      "permanent": true
    },
    {
      "source": "/auth/sign-up/",
      "destination": "/sign-up",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.).*)",
      "destination": "/index.html"
    },
    {
      "source": "/relay-QrJG/static/(.*)",
      "destination": "https://us-assets.i.posthog.com/static/$1"
    },
    {
      "source": "/relay-QrJG/(.*)",
      "destination": "https://us.i.posthog.com/$1"
    }
  ]
}